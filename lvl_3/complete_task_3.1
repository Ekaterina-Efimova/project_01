class Homework:
    def __init__(self, number = 1, height = 10, widht = 10):
        self.number = number
        self.height = height
        self.widht = widht
        self.lists = [[self.number] * self.widht ]* self.height
    def change(self, number):
        self.number = number
        self.lists = [[self.number] * self.widht ]* self.height
        for i in self.lists:
            print(' '.join([str(j) for j in i]))
    def printh(self):
        for i in self.lists:
            print(' '.join([str(j) for j in i]))
    def printhw(self):
        print (f"Height = {self.height}, widht = {self.widht}")
